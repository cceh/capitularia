         <milestone unit="capitulare" n="BK.110"/>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-145v-1" corresp="BK.110_inscriptio"> De
            ILLOS . VIII . OCTO BaN<lb break="no"/>NUS UNDe DOMNUS NOSTeR <lb/> UULT qUOD exaNT
            SOLIDI LX . <cb n="146r"/>
         </ab>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-146r-1" corresp="BK.110_1_inscriptio">
            <seg type="num" n="1">I</seg> . <seg type="numDenom">capitulum</seg> . </ab>
         <ab type="text" xml:id="bamberg-sb-jur-35-146r-2" corresp="BK.110_1">
            <hi rend="coloured">d</hi>ishonora<gap unit="chars" quantity="6"/> ecclesiae ;. <lb/>
         </ab>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-146r-3" corresp="BK.110_2_inscriptio">
            <seg type="num" n="2">II</seg> . <seg type="numDenom">capitulum</seg> . </ab>
         <ab type="text" xml:id="bamberg-sb-jur-35-146r-4" corresp="BK.110_2">
            <hi rend="coloured">q</hi>ui iniuste agit contra uiduas ;. <lb/>
         </ab>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-146r-5" corresp="BK.110_3_inscriptio">
            <seg type="num" n="3">III</seg> . <seg type="numDenom">capitulum</seg> . </ab>
         <ab type="text" xml:id="bamberg-sb-jur-35-146r-6" corresp="BK.110_3">
            <hi rend="coloured">d</hi>e orfanis ;. <lb/>
         </ab>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-146r-7" corresp="BK.110_4_inscriptio">
            <seg type="num" n="4">IIII</seg> . <seg type="numDenom">capitulum</seg> . </ab>
         <ab type="text" xml:id="bamberg-sb-jur-35-146r-8" corresp="BK.110_4">
            contra pauperinus qui se
            <lb/> ipsus defendere non possunt qui di<lb break="no"/>cuntur uruermagon ;. <lb/>
         </ab>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-146r-9" corresp="BK.110_5_inscriptio">
            <seg type="num" n="5">V</seg>
            <seg type="numDenom">capitulum</seg> . </ab>
         <ab type="text" xml:id="bamberg-sb-jur-35-146r-10" corresp="BK.110_5">
            <hi rend="coloured">q</hi>ui raptum facit hoc est qui fe<lb break="no"/>minam ingenuam
            trahit contra uo<lb break="no"/>luntatem parentum<note type="editorial"> Mit Ansatz zu
               einem redundanten Kürzungsstrich über <mentioned>u</mentioned>. </note> suorum ;.
            <lb/>
         </ab>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-146r-11" corresp="BK.110_6_inscriptio">
            <seg type="num" n="6">VI</seg> . <seg type="numDenom">capitulum</seg> . </ab>
         <ab type="text" xml:id="bamberg-sb-jur-35-146r-12" corresp="BK.110_6">
            <hi rend="coloured">q</hi>ui incendium facit infra pa<lb break="no"/>triam hoc est qui
            incendit alterius ca<lb break="no"/>sam aut scuriam ;. <lb/>
         </ab>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-146r-13" corresp="BK.110_7_inscriptio">
            <seg type="num" n="7">VII</seg> . <seg type="numDenom">capitulum</seg> . </ab>
         <ab type="text" xml:id="bamberg-sb-jur-35-146r-14" corresp="BK.110_7">
            <hi rend="coloured">q</hi>ui harizhut facit hoc est <lb/> qui frangit alterius sepem aut
            <lb/> portam aut casam cum uirtute ;. <lb/>
         </ab>

         <ab type="meta-text" xml:id="bamberg-sb-jur-35-146r-15" corresp="BK.110_8_inscriptio">
            <seg type="num" n="8">VII<mod>I</mod></seg> . <seg type="numDenom">capitulum</seg> . </ab>
         <ab type="text" xml:id="bamberg-sb-jur-35-146r-16" corresp="BK.110_8">
            <hi rend="coloured">q</hi>ui in hoste non uadit ;. <lb/>
         </ab>

         <ab type="text" xml:id="bamberg-sb-jur-35-146r-17" corresp="BK.110_epilog">
            <seg type="initial">I</seg>sti sunt octo banni domino regis <lb/> unde exire debent de
            unoquisque <lb/> solido . LX . <lb/>
         </ab>


