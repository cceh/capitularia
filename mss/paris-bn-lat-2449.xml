         <gap reason="editorial"/>

         <cb n="1v"/>
         <lb n="9"/>

         <milestone unit="capitulare" n="BK.259"/>

         <ab type="meta-text" xml:id="paris-bn-lat-2449-1v-2" corresp="BK.259_8_inscriptio"> DE
            MALLIS UEL PLACITIS SĘCULA<lb break="no"/>RIBUS QUIBUS TEMPORIBUS OBSERUARE <lb/> NON
            DEBENT ; <lb/>
         </ab>
         <ab type="text" xml:id="paris-bn-lat-2449-1v-3" corresp="BK.259_8"
            next="#paris-bn-lat-2449-1v-4">
            <seg type="initial">V</seg>o <gap unit="chars" quantity="4"/>s omnibus comitibus
            omnibusque rei publicę <lb/> ministris firmiter denunciare ! ut a quarta feria <lb/>
            ante initium quadragesime nec in ipsa quarta feria . <lb/> usque <subst>
               <del>in</del>
               <add>post</add>
            </subst> octabas paschę mallum uel placitum pub<lb break="no"/>licum . nisi de concordia
            et pacificatione discordan<lb break="no"/>tium . tenere <expan>presumant</expan> ; <lb/>
         </ab>
         <ab type="text" xml:id="paris-bn-lat-2449-1v-4" corresp="BK.259_8"
            prev="#paris-bn-lat-2449-1v-3">
            <seg type="initial">S</seg>imiliter etiam a quarta feria ante natiuitatem domini <lb/>
            usque post consecratos dies obseruent . necnon et in <lb/> ieiuniis quattuor temporum .
            et in rogationibus . <lb/> et in diebus pentecosten . simili obseruatione eos<lb
               break="no"/>dem <unclear>f</unclear><gap unit="chars" quantity="1"/>riatos dies
            uenerari omnimodis studeant ; <lb/>
         </ab>

         <milestone unit="capitulare" n="Mordek.26"/>

         <ab type="meta-text" xml:id="paris-bn-lat-2449-1v-5" corresp="Mordek.26_inscriptio"> UT IN
               <gap unit="chars" quantity="5"/>ITATIBUS ECCLESIARUM MALLUM <lb/> VEL PLACITUM
            PUBLICUM NEMO TENERE PRESUMAT ; <lb/>
         </ab>
         <ab type="text" xml:id="paris-bn-lat-2449-1v-6" corresp="Mordek.26">
            <seg type="initial">PL</seg>acuit . ut nullus comes neque aliquis <lb/> ex iuditiaria
            potestate . mallum aut placitum in immu<cb n="2r" break="no"/>nitatibus uel atriis
            ecclesiarum tenere aut quemquam <lb/> de familia earundem ecclesiarum quacumque
               occasi<lb break="no"/>one aut temeritate iniuriare presumat ; <lb/> Quod si fecerit !
            in tripp<subst>
               <del>o</del>
               <add>lo</add>
            </subst> bannum dominicum <lb/> persoluat ; Et insuper deo et ęcclesię quam lesit .
            secundum <lb/> canonicam institutionem in omnibus satisfaciat ; <lb/> Quod etiam si
            facere distulerit . tamdiu a cętu fide<lb break="no"/>lium arceatur ! donec
               pęnitentia<del unit="chars" quantity="2"/> accepta . reatum <lb/> quem
               <expan>commisit</expan> emendet ; <lb/>
         </ab>

         <gap reason="editorial"/>
         <cb n="6r"/>
         <lb n="21"/>

         <milestone unit="capitulare" n="BK.138"/>

         <ab type="meta-text" xml:id="paris-bn-lat-2449-6r-14" corresp="BK.138_10_inscriptio"> De
            mansis UNIUSCUIUSQUE ĘCCLESIAĘ . <lb/>
         </ab>
         <ab type="text" xml:id="paris-bn-lat-2449-6r-15" corresp="BK.138_10">
            <seg type="initial"
            >S</seg>anccitum<note type="editorial">Am Rand von neuzeitlicher Hand <mentioned>1.91.</mentioned></note>
            est . ut unicuique ęcclesię . unus mansus integer . <lb/> absque aliquo seruitio
            attribuatur . Et <expan>presbiteri</expan> . in eis <lb/> constituti . non de decimis 
            neque de oblationibus fide<lb break="no"/>lium . non
            de domibus neque de atriis uel hortis iuxta <lb/> ęcclesiam positis . neque de
            pręscripto manso aliquod <cb n="6v"/> seruitium faciant . praeter ęcclesiasticum ; Et si
            aliquid <lb/> amplius habuerint ! inde senioribus suis debitum <lb/> seruitium inpendant
            . <lb/>
            <!--  sk: ähnelt am ehesten Gotha I.84, aber einmal auch näher zu Paris 2400/4280a (aliquo statt alio)
            Mit dieser einen Ausnahme (sowie belanglosem ortis/hortis) könnte die Fassung sonst aus 
            Ansegis stammen; Schmitz S. 282 erwähnt diese Fassung aber nicht, sondern nur eine
            weitere Fasung auf fol. 54v; dort allerdings auch "absque aliquo"...   
            -->
         </ab>


         <gap reason="editorial"/>
         <cb n="55v"/>
         <lb n="10"/>

         <milestone unit="capitulare" n="Mordek.21"/>


         <ab type="meta-text" xml:id="paris-bn-lat-2449-55v-1" corresp="Mordek.21_inscriptio"> ITEM
            DE NONIS ET DECIMIS <lb/>
         </ab>
         <ab type="text" xml:id="paris-bn-lat-2449-55v-2" corresp="Mordek.21" rend="nota">
            <expan><seg type="initial">Q</seg>uicumque</expan> nonam et decimam
            ammonente episcopo uel abbate uel <lb/> quolibet alio rectore
            ad primam eius <expan>ammonitionem</expan> da<lb break="no"/>re consenserit . regia
            potestate cogente easdem deci<lb break="no"/>mas et nonas soluat .
