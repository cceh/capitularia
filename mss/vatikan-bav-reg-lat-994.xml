         <cb n="1v"/>
         <lb n="8"/>

         <milestone unit="capitulare" n="BK.259"/>

         <ab type="meta-text" xml:id="vatikan-bav-reg-lat-994-1v-1" rend="coloured"
            corresp="BK.259_8_inscriptio">DE MALLIS UEL PLACITIS SAECULARIBUS QUIBUS <lb/>
            TEMPORIBUS OBSERUARI NON DEBEANT <lb/>
         </ab>

         <ab type="text" xml:id="vatikan-bav-reg-lat-994-1v-2" corresp="BK.259_8"><seg
               type="initial" rend="coloured">V</seg>olumus omnibus comitibus omnibusque rei pub<lb
               break="no"/>licae ministris firmiter denuntiare . ut a quarta <lb/> feria ante
            initium quadragesimae . nec in ipsa quar<lb break="no"/>ta feria usque post octauas
            paschae . mallum uel placitum <lb/>
            <subst>
               <del>pl</del>
               <add>publicum</add>
            </subst> nisi de concordia et pacificatione discordantium <lb/> tenere praesumant .
            Similiter <add>etiam <del>a</del></add><metamark function="insertion"/><note
               type="editorial"><mentioned>etiam</mentioned> über der Zeile mit Einfügungszeichen
               ergänzt, nach <mentioned>etiam</mentioned> steht, ebenfalls über der Zeile, getilgtes
                  <mentioned>a</mentioned></note> quarta feria ante natiui<lb break="no"/>tatem
            domini . usque post consecratos dies obseruent . necnon <lb/> et in ieiuniis quattuor
            temporum . et in rogationibus . et in <lb/> diebus pentecostes . simili obseruatione
            eosdem feriatos <lb/> dies uenerari omnimodis studeant . <lb/>
         </ab>

         <milestone unit="capitulare" n="Mordek.26"/>

         <ab type="meta-text" xml:id="vatikan-bav-reg-lat-994-1v-3" rend="coloured"
            corresp="Mordek.26_inscriptio">UT IN IMMUNITATIBUS ECCLESIARUM MALLUM UEL <lb/> PLACITUM
            PUBLICUM NEMO TENERE <expan>PRAESUMAT</expan> . <lb/>
         </ab>

         <ab type="text" xml:id="vatikan-bav-reg-lat-994-1v-4" corresp="Mordek.26"><seg
               type="initial" rend="coloured">P</seg>lacuit ut nullus comes . neque aliquis ex
            iudiciaria <cb n="2r"/> potestate . mallum aut placitum publicum in
            <mod>im</mod>munitatibus uel <lb/> atriis ecclesiarum tenere . aut quemquam de familia
            earundem <lb/> ecclesiarum . quacumque occasione aut temeritate iniuriare <lb/>
            praesumat . Quod si fecerit . in triplo bannum dominicum persol<lb break="no"/>uat . et
            insuper deo et ecclesiae quam lęsit . secundum canonicam <lb/> institutionem in omnibus
            satisfaciat . Quod etiam si facere <lb/> distulerit . tamdiu a co<subst>
               <del/>
               <add>e</add>
            </subst>tu fidelium arceatur . donec poeni<lb break="no"/>tentia accepta reatum quem
            commisit . emendet . <lb/>
         </ab>

         <gap reason="editorial"/>
         <cb n="51v"/>
         <lb n="3"/>

         <ab type="meta-text" xml:id="vatikan-bav-reg-lat-994-51v-2" rend="coloured">INCIPIT DE
            REBUS ECCLESIAE <lb/>
         </ab>

         <gap reason="editorial"/>
         <cb n="52v"/>
         <lb n="8"/>

         <milestone unit="capitulare" n="Mordek.21"/>

         <ab type="meta-text" xml:id="vatikan-bav-reg-lat-994-52v-1" corresp="Mordek.21_inscriptio"
            rend="coloured"><seg type="num" n="4">IIII</seg> ITEM DE NONIS ET DECIMIS . <lb/>
         </ab>

         <ab type="text" xml:id="vatikan-bav-reg-lat-994-52v-2" corresp="Mordek.21"><expan><seg
                  type="initial" rend="coloured">Q</seg>uicumque</expan> nonam et decimam a<subst>
               <del>m</del>
               <add hand="X">d</add>
            </subst>monente episcopo uel abbate <lb/> uel quolibet alio rectore ad primam eius
            admonitionem <lb/> dare contempserit . regia potestate cogente easdem <lb/> decimas et
            nonas soluat . et insuper ad pa<subst>
               <del>trem</del>
               <add>rtem</add>
            </subst> . eiusdem <lb/> ecclesiae legem suam conponat . Si secundo admonitus <lb/>
            praedictas nonas et decimas reddere neglexerit . cum <lb/> eisdem nonis et decimis
            bannum dominicum persoluat . <lb/> Nam si tertio admonitus in eisdem nonis et decimis
            <lb/> dandis neglegens euenerit . res ecclesiae de quibus easdem <lb/> nonas et decimas
            dare debuerat penitus amittat . <lb/> Nonae uero et decimae tantum de annona . legumine
            <lb/> uino feno . et nutrimine omnis generis animalium <lb/> dandae sunt . Et hi qui
            nonas et decimas dare debent . <lb/> iuxta quod rerum ecclesiasticarum habent . ad
            ecclesias <cb n="53r"/> et <subst>
               <del>castra</del>
               <add>claustra</add>
            </subst> rectorum uenerabilium locorum adiutorium faciant . <lb/>
         </ab>

