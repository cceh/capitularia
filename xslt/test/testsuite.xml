<?xml version="1.0" encoding="UTF-8"?>
<TEI xml:lang="de" xmlns="http://www.tei-c.org/ns/1.0">
  <text>
    <body>
      <ab type="meta-text" rend="coloured">
        before new* after / * new ergänzt
      </ab>
      <ab type="text">
        before <add>new</add> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before new* new2* after / * new ergänzt / * new2 ergänzt
      </ab>
      <ab type="text">
        before <add>new</add> <add>new2</add> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before new* after / * e ergänzt
      </ab>
      <ab type="text">
        before n<add>e</add>w after
      </ab>

      <ab type="meta-text" rend="coloured">
        before newer* after / * e² ergänzt
      </ab>
      <ab type="text">
        before new<add>e</add>r after
      </ab>

      <ab type="meta-text" rend="coloured">
        before newer* after / * e¹ ergänzt, e² ergänzt
      </ab>
      <ab type="text">
        before n<add>e</add>w<add>e</add>r after
      </ab>

      <ab type="meta-text" rend="coloured">
        before conuertantur* after / * gek. cutat, conuertantur ergänzt
      </ab>
      <ab type="text">
        before <add><choice>
                      <abbr>cutat</abbr>
                      <expan>conuertantur</expan>
                      </choice></add> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before conuertantur* after / * conuertantur ergänzt, gek. cutat
      </ab>
      <ab type="text">
        before <add><choice>
                      <abbr>cutat</abbr>
                      <expan>conuertantur</expan>
                      </choice></add><note type="editorial">conuertantur ergänzt, gek. cutat</note> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before* after / * folgt von Hand X ergänztes new
      </ab>
      <ab type="text">
        before <add hand="X">new</add> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before nw* after / * von Hand X korr. zu new
      </ab>
      <ab type="text">
        before n<add hand="X">e</add>w after
      </ab>

      <ab type="meta-text" rend="coloured">
        before* after / * folgt von Hand X ergänztes conuertantur, gek. cutat
      </ab>
      <ab type="text">
        before <add hand="X"><choice>
                      <abbr>cutat</abbr>
                      <expan>conuertantur</expan>
                    </choice></add> after

      </ab>

      <!-- del -->

      <ab type="meta-text" rend="coloured">
        before* after / * folgt getilgtes new
      </ab>
      <ab type="text">
        before <del>new</del> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before new* after / * korr. aus neew
      </ab>
      <ab type="text">
        before ne<del>e</del>w after
      </ab>

      <ab type="meta-text" rend="coloured">
        before* after / * folgt getilgtes [+]
      </ab>
      <ab type="text">
        before <del></del> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before* after / * folgt getilgtes conuertantur, gek. cutat
      </ab>
      <ab type="text">
        before <del><choice>
                      <abbr>cutat</abbr>
                      <expan>conuertantur</expan>
                    </choice></del> after
      </ab>

      <ab type="meta-text" rend="coloured">
        quocunque* / * korr. aus quo+cunque
      </ab>
      <ab type="text">
        quo<del quantity="1" unit="chars"/>cunque
      </ab>


      <ab type="meta-text" rend="coloured">
        before new* after / * von Hand X getilgt
      </ab>
      <ab type="text">
        before <del hand="X">new</del> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before neew* after / * von Hand X korr. zu new
      </ab>
      <ab type="text">
        before ne<del hand="X">e</del>w after
      </ab>

      <ab type="meta-text" rend="coloured">
        before [+]* after / * von Hand X getilgt
      </ab>
      <ab type="text">
        before <del hand="X"></del> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before conuertantur* after / * gek cutat, von Hand X getilgt
      </ab>
      <ab type="text">
        before <del hand="X"><choice>
                      <abbr>cutat</abbr>
                      <expan>conuertantur</expan>
                    </choice></del> after
      </ab>

      <!-- subst -->

      <ab type="meta-text" rend="coloured">
        before new* after / * korr. aus old
      </ab>
      <ab type="text">
        before <subst><del>old</del><add>new</add></subst> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before new* after / * korr. aus now
      </ab>
      <ab type="text">
        before n<subst><del>o</del><add>e</add></subst>w after
      </ab>

      <ab type="meta-text" rend="coloured">
        before new new* after / * new new korr. aus old old
      </ab>
      <ab type="text">
        before <subst><del>old old</del><add>new new</add></subst> after
      </ab>


      <ab type="meta-text" rend="coloured">
        before old* after / * von Hand X korr. zu new
      </ab>
      <ab type="text">
        before <subst><del>old</del><add hand="X">new</add></subst> after
      </ab>

      <ab type="meta-text" rend="coloured">
        before now* after / * von Hand X korr. zu new
      </ab>
      <ab type="text">
        before n<subst><del>o</del><add hand="X">e</add></subst>w after
      </ab>

      <ab type="meta-text" rend="coloured">
        before old old* after / * old old von Hand X korr. zu new new
      </ab>
      <ab type="text">
        before <subst><del>old old</del><add hand="X">new new</add></subst> after
      </ab>

      <ab type="meta-text" rend="coloured">
        perdic[fol. 17r]tum* / * von Hand X korr. zu praedictum
      </ab>
      <ab type="text">
        <subst>
          <add hand="X">prae</add>
          <del>per</del>
        </subst>dic<cb n="17r" break="no"/>tum
      </ab>

    </body>
  </text>
</TEI>
